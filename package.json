    "build:minimal": "node build-minimal-fixed.js",
    "build:bypass": "node build-bypass-fixed.js",
    "build:fixed": "node build-fixed.js",
    "build:aggressive": "node build-aggressive.js",
    "build:permanent": "node build-permanent-fix.js",
