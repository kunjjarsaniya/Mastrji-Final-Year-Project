    "build:fallback": "next build",
    "fix-build": "node fix-build.js",
    "fix-windows": "powershell -ExecutionPolicy Bypass -File fix-windows-build.ps1",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "diagrams:svg": "mmdc -p node_modules/puppeteer/.local-chromium/*/chrome-win/chrome.exe -i docs/diagrams/system-design.mmd -o docs/diagrams/system-design.svg && mmdc -i docs/diagrams/use-case.mmd -o docs/diagrams/use-case.svg && mmdc -i docs/diagrams/er.mmd -o docs/diagrams/er.svg && mmdc -i docs/diagrams/dfd-level-0.mmd -o docs/diagrams/dfd-level-0.svg && mmdc -i docs/diagrams/dfd-level-1.mmd -o docs/diagrams/dfd-level-1.svg && mmdc -i docs/diagrams/dfd-level-2.mmd -o docs/diagrams/dfd-level-2.svg && mmdc -i docs/diagrams/activity-login.mmd -o docs/diagrams/activity-login.svg && mmdc -i docs/diagrams/activity-enrollment.mmd -o docs/diagrams/activity-enrollment.svg && mmdc -i docs/diagrams/architecture.mmd -o docs/diagrams/architecture.svg",
    "diagrams:png": "mmdc -i docs/diagrams/system-design.mmd -o docs/diagrams/system-design.png && mmdc -i docs/diagrams/use-case.mmd -o docs/diagrams/use-case.png && mmdc -i docs/diagrams/er.mmd -o docs/diagrams/er.png && mmdc -i docs/diagrams/dfd-level-0.mmd -o docs/diagrams/dfd-level-0.png && mmdc -i docs/diagrams/dfd-level-1.mmd -o docs/diagrams/dfd-level-1.png && mmdc -i docs/diagrams/dfd-level-2.mmd -o docs/diagrams/dfd-level-2.png && mmdc -i docs/diagrams/activity-login.mmd -o docs/diagrams/activity-login.png && mmdc -i docs/diagrams/activity-enrollment.mmd -o docs/diagrams/activity-enrollment.png && mmdc -i docs/diagrams/architecture.mmd -o docs/diagrams/architecture.png"
  },
